import java.util.Scanner;
public class Main{
    public static void main(String[] args){
        Scanner sc=new Scanner(System.in);
        int num=sc.nextInt();
        int maxdigit=0;
        int temp=num;
        while(temp>0){
            int digit=temp%10;
            if(digit>maxdigit){
                maxdigit=digit;
            }
        temp=temp/10;
    }
    System.out.println(maxdigit);
}
}
