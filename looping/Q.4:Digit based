
import java.util.Scanner;
public class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int num = sc.nextInt();
        int digit = sc.nextInt();
        int count = 0;
        while (num > 0) {
            int lastDigit = num % 10;
            if (lastDigit == digit) {
                count++;
            }
            num = num / 10;
        }
        System.out.println(count);
    }
}
