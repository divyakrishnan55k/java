import java.util.Scanner;
public class Main{
    public static void main(String[] args){
        Scanner sc=new Scanner(System.in);
        int num=sc.nextInt();
        int mindigit=1;
        int temp=num;
        while(temp>0){
            int digit=temp%10;
            if(digit<mindigit){
                mindigit=digit;
            }
        temp=temp/10;
    }
    System.out.println(mindigit);
}
}
