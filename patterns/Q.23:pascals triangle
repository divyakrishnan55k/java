import java.util.Scanner;
public class Main{
    public static void main(String[] args){
        Scanner sc=new Scanner(System.in);
        int n=sc.nextInt();
        for(int i=1;i<=n;i++){
            for(int s=0;s<=n-i;s++){
                System.out.print(" ");
            }
            int num=1;
            for(int j=1;j<=n;j++){
                if(i>=j){
                    System.out.print(num+" ");
                    num=num*(i-j)/j;
            }
            }
            System.out.println();
            }
    }
}
