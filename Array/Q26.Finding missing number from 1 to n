import java.util.Scanner;
public class Main{
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);

        int n = sc.nextInt(); // range 1 to n
        int[] arr = new int[n - 1];

        for (int i = 0; i < n - 1; i++)
            arr[i] = sc.nextInt();

        int total = n * (n + 1) / 2;
        int sum = 0;

        for (int x : arr)
            sum += x;

        System.out.println("Missing number = " + (total - sum));
    }
}
