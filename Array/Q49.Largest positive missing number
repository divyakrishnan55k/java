import java.util.*;
public class Main{
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);

        int n = sc.nextInt();
        Set<Integer> set = new HashSet<>();

        for (int i = 0; i < n; i++)
            set.add(sc.nextInt());

        int max = Collections.max(set);

        for (int i = max; i >= 1; i--) {
            if (!set.contains(i)) {
                System.out.println(i);
                return;
            }
        }
        System.out.println("No missing number");
    }
}
